    "@types/node": "^18.16.3",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.15",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.39.0",
